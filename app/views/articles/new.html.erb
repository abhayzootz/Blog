<h1>New article</h1>
<%= form_for :article, url: articles_path, method: "POST" do |f| %>
  <p>
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </p>
 
  <p>
    <%= f.label :text %><br>
    <%= f.text_area :text %>
  </p>
  <p>
  <%= f.label :chiu %><br>
  <%= f.text_area :address %>
  </p>
  <p>
    <%= f.submit %>
  </p>
<% end %>